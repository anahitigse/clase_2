    {
      "cell_type": "code",
      "source": [
        "list_1 = [1,2,3,4]\n",
        "result = []\n",
        "while list_1:\n",
        "  element = list_1.pop(0)\n",
        "  if element % 2 == 0:\n",
        "    result.append(element ** 2)\n",
        "print(result)"
